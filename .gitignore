# Ignore Composer's vendor directory
/vendor/

# Ignore environment files
.env
.env.local
.env.*

# Ignore log files and caches
*.log
*.cache

# Ignore IDE/editor-specific files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore node modules if front-end uses npm
node_modules/

# Ignore PHPUnit test results
.phpunit.result.cache

# Ignore compiled templates or views
/storage/framework/views/*
/storage/logs/*

# Ignore user file uploads if present
/public/uploads/
